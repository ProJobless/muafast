<?xml version="1.0"?><config>    <modules>        <FavoredMinds_Vendor>            <version>0.1.3</version>        </FavoredMinds_Vendor>    </modules>    <frontend>        <routers>            <vendor>                <use>standard</use>                <args>                    <module>FavoredMinds_Vendor</module>                    <frontName>vendor</frontName>                </args>            </vendor>        </routers>        <layout>            <updates>                <vendor>                    <file>favoredminds_vendor.xml</file>                </vendor>            </updates>        </layout>    </frontend>    <admin>        <routers>            <vendor>                <use>admin</use>                <args>                    <module>FavoredMinds_Vendor</module>                    <frontName>vendor</frontName>                </args>            </vendor>        </routers>    </admin>    <adminhtml>        <menu>            <favoredminds_vendor module="vendor">                <title>Vendors</title>                <sort_order>71</sort_order>                <children>		  <!-- Start of block by Mudassar -->                    <info module="vendor">                        <title>Vendor Information</title>                        <sort_order>0</sort_order>                        <action>vendor/adminhtml_information</action>                    </info>		  <!-- End of block by Mudassar -->                    <view module="vendor">                        <title>Vendors</title>                        <sort_order>1</sort_order>                        <action>vendor/adminhtml_vendors</action>                    </view>                    <sales module="vendor">                        <title>Vendor Orders</title>                        <sort_order>4</sort_order>                        <action>vendor/adminhtml_sales</action>                    </sales>     
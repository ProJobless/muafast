  <vendor_sales>                        <table>vendors_sales</table>                    </vendor_sales>                    <vendor_sales_items>                        <table>vendors_sales_items</table>                    </vendor_sales_items>                                        <vendor_customer_rating>                        <table>customer_rating</table>                    </vendor_customer_rating>                        <vendor_sales_history>                        <table>vendors_sales_history</table>                    </vendor_sales_history>                    <vendor_statement>                        <table>vendors_statements</table>                    </vendor_statement>                </entities>            </vendor_mysql4>        </models>        <template>            <email>                <vendor_email_newordertemplate translate="label" module="vendor">                    <label>Vendor New Order</label>                    <file>vendor/order_new.html</file>                    <type>html</type>                </vendor_email_newordertemplate>                <vendor_email_newvendortemplate translate="label" module="vendor">                    <label>Vendor Signup</label>                    <file>vendor/vendor_new.html</file>                    <type>html</type>                </vendor_email_newvendortemplate>                <vendor_email_statement_template translate="label" module="vendor">                    <label>Vendor Statement</label>                    <file>vendor/statement_new.html</file>                    <type>html</type>                </vendor_email_statement_template>            </email>        </template>        <resources>            <vendor_setup>                <setup>                    <module>FavoredMinds_Vendor</module>                </setup>                <connection>                    <use>core_setup</use>                </connection>            </vendor_setup>            <vendor_write>                <connection>                    <use>core_write</use>                </connection>            </vendor_write>            <vendor_read>                <connection>                    <use>core_read</use>                </connection>            </vendor_read>        </resources>        <blocks>            <vendor>                <class>FavoredMinds_Vendor_Block</class>            </vendor>            <adminhtml>                <rewrite>                                        <page_menu>FavoredMinds_Vendor_Block_Adminhtml_Page_Menu</page_menu>                    <catalog_product_grid>FavoredMinds_Vendor_Block_Catalog_Product_Grid</catalog_product_grid>                    <notification_window>FavoredMinds_Vendor_Block_Adminhtml_Block_Notification_Window</notification_window>                                        <notification_toolbar>FavoredMinds_Vendor_Block_Adminhtml_Block_Notification_Toolbar</notification_toolbar>                    <sales_order_view>FavoredMinds_Vendor_Block_Adminhtml_Order_View</sales_order_view>                    <sales_order_invoice_create>FavoredMinds_Vendor_Block_Adminhtml_Order_Invoice_Create</sales_order_invoice_create>                    <sales_order_invoice_view>FavoredMinds_Vendor_Block_Adminhtml_Order_Invoice_View</sales_order_invoice_view>                </rewrite>            </adminhtml>        </blocks>        <helpers>            <vendor>                <class>FavoredMinds_Vendor_Helper</class>            </vendor>        </helpers>    </global></config>
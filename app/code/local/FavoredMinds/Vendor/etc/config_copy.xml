   <sort_order>11</sort_order>                                </sales>                                <statements>                                    <title>Statements</title>                                    <sort_order>12</sort_order>                                </statements>                                <setup module="vendor">                                    <title>Setup</title>                                    <sort_order>99</sort_order>                                </setup>				<!-- Start of block by Mudassar -->                                <info module="vendor">                                    <title>Vendor Information</title>                                    <sort_order>100</sort_order>                                </info>				<!-- End of block by Mudassar -->                            </children>                        </favoredminds_vendor>                        <report>                            <children>                                <salesroot>                                    <children>                                        <favoredminds_vendor_report_sales>                                            <title>FavoredMinds Vendor Sales Report</title>                                            <sort_order>11</sort_order>                                        </favoredminds_vendor_report_sales>                                    </children>                                </salesroot>                            </children>                        </report>                    </children>                </admin>            </resources>        </acl>        <layout>            <updates>                <vendor>                    <file>favoredminds_vendor.xml</file>                </vendor>            </updates>        </layout>        <events>            <catalog_product_new_action>                <observers>                    <vendor>        
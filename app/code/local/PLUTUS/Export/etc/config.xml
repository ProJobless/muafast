<?xml version="1.0"?><config>    <modules>        <PLUTUS_Export>            <version>0.1.0</version>        </PLUTUS_Export>    </modules>    <frontend>        <routers>            <export>                <use>standard</use>                <args>                    <module>PLUTUS_Export</module>                    <frontName>export</frontName>                </args>            </export>        </routers>        <layout>            <updates>                <export>                    <file>plutus_export.xml</file>                </export>            </updates>        </layout>    </frontend>    <admin>        <routers>            <export>                <use>admin</use>                <args>                    <module>PLUTUS_Export</module>                    <frontName>export</frontName>                </args>            </export>        </routers>    </admin>    <adminhtml>        <menu>            <plutus_export module="export">                <title>Export Attribute</title>                <sort_order>71</sort_order>            </plutus_export>       </menu>           </adminhtml>        <global>                <template>        </template>        <resources>            <export_setup>                <setup>                    <module>PLUTUS_Export</module>                </setup>                <connection>                    <use>core_setup</use>                </connection>            </export_setup>            <export_write>                <connection>                    <use>core_write</use>                </connection>            </export_write>            <export_read>                <connection>                    <use>core_read</use>                </connection>            </export_read>        </resources>                <blocks>            <export>                <class>PLUTUS_Export_Block</class>            </export>                    </blocks>        <helpers>            <export>                <class>PLUTUS_Export_Helper</class>            </export>        </helpers>    </global></config>